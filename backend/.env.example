# Database URL: SQLite for local dev, PostgreSQL for production
# Local: DATABASE_URL="file:./dev.db"
# Production: DATABASE_URL="postgresql://user:password@host:5432/dbname"
DATABASE_URL=

# JWT secret for signing tokens (required)
JWT_SECRET=

# Optional: server port
PORT=

# Optional: for db:seed demo user only (omit in production)
DEMO_USER_EMAIL=
DEMO_USER_PASSWORD=
